{
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install --prefix frontend && npm install --prefix backend",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}
